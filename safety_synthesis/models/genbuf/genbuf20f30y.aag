      sys_fair17done = sys_fair17done | sys_fair17;
      sys_fair18done = sys_fair18done | sys_fair18;
      sys_fair19done = sys_fair19done | sys_fair19;
      sys_fair20done = sys_fair20done | sys_fair20;
      if(all_env_fair_fulfilled)
       begin
         env_fair0done = 0;
         env_fair1done = 0;
         fair_cnt = fair_cnt + 1;
       end
      else
       begin
         env_fair0done = env_fair0done | env_fair0;
         env_fair1done = env_fair1done | env_fair1;
       end
    end

