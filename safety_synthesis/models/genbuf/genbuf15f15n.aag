   reg_i_StoB_REQ12 =  i_StoB_REQ12;
   reg_controllable_BtoS_ACK12 =  controllable_BtoS_ACK12;
   reg_i_StoB_REQ13 =  i_StoB_REQ13;
   reg_controllable_BtoS_ACK13 =  controllable_BtoS_ACK13;
   reg_i_StoB_REQ14 =  i_StoB_REQ14;
