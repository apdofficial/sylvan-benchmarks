// G((FULL=1 * DEQ=0) -> ENQ=0);	#G11
assign sys_safe_err93 = ~(~(i_FULL & ~controllable_DEQ) | ~controllable_ENQ);

// G(EMPTY=1 -> DEQ=0);	#G11
assign sys_safe_err94 = ~( i_nEMPTY | ~controllable_DEQ);

