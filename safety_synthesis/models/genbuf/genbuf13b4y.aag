   reg_i_StoB_REQ11 =  i_StoB_REQ11;
   reg_controllable_BtoS_ACK11 =  controllable_BtoS_ACK11;
   reg_i_StoB_REQ12 =  i_StoB_REQ12;
   reg_controllable_BtoS_ACK12 =  controllable_BtoS_ACK12;
   reg_i_RtoB_ACK0 =  i_RtoB_ACK0;
